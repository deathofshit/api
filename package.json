{
  "name": "@eksisozluk/api",
  "version": "0.2.2",
  "description": "kutsal bilgi kaynağı (unofficial)",
  "homepage": "https://eksisozluk.herokuapp.com/",
  "private": true,
  "author": {
    "name": "Gökay Gürcan",
    "email": "info@gokaygurcan.com",
    "url": "https://www.gokaygurcan.com/"
  },
  "copyright": "2016",
  "license": "GPL-3.0",
  "main": "index.js",
  "repository": "eksisozluk/api",
  "bugs": {
    "url": "https://github.com/eksisozluk/api/issues"
  },
  "scripts": {
    "start": "node index.js",
    "test": "xo",
    "postpublish": "greenkeeper-postpublish",
    "ab": "ab -n 5000 -c 100 http://localhost:3000/"
  },
  "keywords": [
    "ekşi",
    "sözlük",
    "api"
  ],
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "eksisozluk": "^0.3.0",
    "express": "^4.16.4",
    "express-hbs": "^2.0.2",
    "express-session": "^1.15.6",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "node-sass-middleware": "^0.11.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "serve-favicon": "^2.5.0",
    "toobusy-js": "^0.5.1"
  },
  "devDependencies": {
    "greenkeeper-postpublish": "^2.0.0",
    "xo": "^0.24.0"
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "rules": {
      "new-cap": 0,
      "brace-style": [
        "error",
        "stroustrup",
        {
          "allowSingleLine": true
        }
      ],
      "capitalized-comments": 0,
      "unicorn/catch-error-name": [
        "error",
        {
          "name": "err"
        }
      ],
      "prefer-destructuring": 0
    }
  },
  "greenkeeper": {
    "ignore": []
  }
}
